createdb movie_library;

CREATE TABLE MOVIES (
id SERIAL PRIMARY KEY,
Name TEXT NOT NULL,
Actor TEXT NOT NULL,
ACTRESS TEXT NOT NULL,
Director TEXT,
Year_released INT);

